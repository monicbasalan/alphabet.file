(milestoneElem) === true) && notScrolledEndFirstMethod) {\n    window['optimizely'] = window['optimizely'] || [];\n    window.optimizely.push([\"trackEvent\", \"scroll_end_first_method\"]);\n    //logMachinify({\"scroll_distance\":scrolledToDistance(milestoneElem), \"time_on_page\":curTime, \"event\":\"scroll_end_first_method\"});\n    notScrolledEndFirstMethod = false;\n\t}\n  milestoneElem = $(\".final_li\");\n  if ((scrolledCompletelyIntoView(milestoneElem) === true) && notScrolledEndLastStep) {\n    window['optimizely'] = window['optimizely'] || [];\n    window.optimizely.push([\"trackEvent\", \"scroll_end_last_step\"]);\n    //logMachinify({\"scroll_distance\":scrolledToDistance(milestoneElem), \"time_on_page\":curTime, \"event\":\"scroll_end_last_step\"});\n    notScrolledEndLastStep = false;\n\t}\n});\n\n$(\".mwimg.largeimage:gt(0)\").hide();"},"7828994938":{"code":"var answerCount = 0;\n\n$(\".qat_cat_chooser\").after(\"<div class='sidebox qat_signup'><h3 class='sidebox-header'>Join Us</h3><div class='qat_signup_details'><div class='signup_message'>You have a lot of great information to share!<br><br>Would you like to create an account to get credit for your answers and unlock more ways to contribute?</div><a class='primary button sign-up' href='https://www.wikihow.com/index.php?title=Special:UserLogin&type=signup&returnto=Special:AnswerQuestions'>Sign Up</a><a class='signup-close'>No thanks</a></div></div>\");\n\n$(\".qat_cat_chooser\").after(\"<div class='sidebox qat_answercount'><h3 class='sidebox-header'>Your Answers This Session</h3><div class='qat_answercount_details'><div class='answer_count'>0</div><div class='answer_count_message'>Answer a question to get started!</div></div></div>\");\n\n$(document).on(\"click\", \".qat_submit\", function(){\n  if ($(this).siblings(\".qat_answer\").val() !== \"\") {\t\n    answerCount += 1;\n    var newMessage = getMessage(answerCount);\n    $(\".answer_count\").text(answerCount);\n    $(\".answer_count_message\").